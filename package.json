    "enzyme-adapter-react-16": "^1.15.2",
    "formik": "^2.1.4",
    "framer-motion": "^1.11.0",
    "mocha": "^7.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0",
    "react-scroll": "^1.7.16",
    "yup": "^0.28.5"
