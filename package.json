    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "autoprefixer": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-import-resolver-webpack": "*",
    "eslint-plugin-import": "*",
